<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RestService"
                   namespace="RestService.Dominio"
                   default-lazy="false">

	<class name="Odontologo" table="TOdontologo">
		<id name="Codigo" column="Codigo">
			<generator class="increment"/>
		</id>

		<property name="Nombres" column="Nombres"></property>
		<property name="ApePaterno" column="ApellidoPaterno"></property>
		<property name="MatPaterno" column="ApellidoMaterno"></property>
		<property name="Sexo" column="Sexo"></property>
		<property name="TipoDocumento" column="TipoDocumento"></property>
		<property name="NroDocumento" column="NumeroDocumento"></property>
		<property name="Correo" column="Correo"></property>
		<property name="Contrasena" column="Contrasena"></property>
		<property name="COP" column="COP"></property>
		<property name="CodigoEspecialidad" column="CodigoEspecialidad"></property>

	</class>

</hibernate-mapping>