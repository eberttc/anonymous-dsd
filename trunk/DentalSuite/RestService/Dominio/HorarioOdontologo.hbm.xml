<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RestService"
                   namespace="RestService.Dominio"
                   default-lazy="false">

	<class name="HorarioOdontologo" table="THorarioOdontologo">
		<id name="Codigo" column="Codigo">
			<generator class="increment"/>
		</id>		
		<property name="CodigoOdontologico" column="CodigoTOdontologo"></property>
		<property name="CodigoHorario" column="CodigoTHorario"></property>
		<property name="FechaCita" column="FechaCita"></property>
		<property name="Estado" column="Estado"></property>


	</class>

</hibernate-mapping>